{
  "name": "template-next-static-contentful",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tscheck": "tsc --noEmit",
    "analyse": "cross-env ANALYZE=true next build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "codegen:generate": "graphql-codegen -r dotenv/config --config src/libs/codegen/codegen.ts",
    "codegen:watch": "graphql-codegen --watch -r dotenv/config --config src/libs/codegen/codegen.ts",
    "codegen:check": "graphql-codegen --check -r dotenv/config --config src/libs/codegen/codegen.ts",
    "plop:atomic-component": "plop atomic-component --plopfile plop/plopfile.js",
    "plop:contentful-component": "plop contentful-component --plopfile plop/plopfile.js",
    "generate:atomic": "pnpm plop:atomic-component",
    "generate:contentful": "pnpm plop:contentful-component",
    "generate:graphql": "pnpm codegen:generate"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.3",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "clsx": "^1.2.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "graphql": "^16.6.0",
    "hoist-non-react-statics": "^3.3.2",
    "next": "13.4.3",
    "next-intl": "^2.14.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@graphql-codegen/cli": "4.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@next/bundle-analyzer": "^13.4.3",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/nextjs": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/react": "^14.0.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "plop": "^3.1.2",
    "prettier": "2.8.8",
    "storybook": "^7.0.18",
    "unplugin-auto-import": "^0.16.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1"
  }
}
